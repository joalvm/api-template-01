{
    "name": "template/api",
    "type": "project",
    "description": "Template for Laravel API",
    "keywords": [],
    "license": "MIT",
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "phpcs": "phpcs --standard=ruleset.xml",
        "phpcs-fixer": "phpcbf --standard=ruleset.xml --report=diff",
        "cs-fixer": "php-cs-fixer fix . --using-cache=no --config=.php_cs.php"
    },
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/agent": "^2.6",
        "joalvm/backend-utils": "^2.0",
        "laravel/framework": "^10.10",
        "spatie/laravel-data": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.17",
        "laravel/tinker": "^2.8",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/utils.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
    ]
}
